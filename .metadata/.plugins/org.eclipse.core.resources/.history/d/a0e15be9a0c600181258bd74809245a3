package javabrushup;

public class FindMatchingElements {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int []list={21,3,13,44,5};
		int num=5;
		
	int a;
	int c;
		for(a=0;a<5;a=a+1){
			if(list[a]==num){
				System.out.println("true");
			}
			else{
				System.out.println("false");
			}
		}
		
	}

}
