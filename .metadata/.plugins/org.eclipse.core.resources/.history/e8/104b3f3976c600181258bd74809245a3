package javabrushup;

public class PositiveandNegative {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=10;
		
		if (a>0){
			System.out.println("Its Positive");
		}
		else if (a<0){
			System.out.println("Its Negative");
		}
		else if (a==0){
			System.out.println("Zero");
		}
	}

}
